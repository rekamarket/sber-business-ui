let make = (. ~size: LimitSize.t, ~color: LimitColor.t) => switch (size) {
| #xl => switch (color) {
  | #light => icon.xl.light
  | #dark  => icon.xl.dark
  }
| #l  => switch (color) {
  | #light => icon.l.light
  | #dark  => icon.l.dark
  }
| #m  => switch (color) {
  | #light => icon.m.light
  | #dark  => icon.m.dark
  }
| #s  => switch (color) {
  | #light => icon.s.light
  | #dark  => icon.s.dark
  }
}
